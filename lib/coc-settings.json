{
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "svelte"
  ],
  "git.addGBlameToVirtualText": true,
  "list.normalMappings": {
    "t": "action:tabe",
    "v": "action:vsplit",
    "s": "action:split"
  },
  "list.insertMappings": {
    "<C-t>": "action:tabe",
    "<C-v>": "action:vsplit",
    "<C-s>": "action:split",
    "<C-w>": "command:wincmd k"
  },
  "python.formatting.provider": "yapf",
  "snippets.extends": {
    "javascriptreact": [
      "javascript"
    ]
  }
}
